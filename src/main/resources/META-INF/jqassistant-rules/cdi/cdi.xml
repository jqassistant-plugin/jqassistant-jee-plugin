<jqassistant-rules xmlns="http://schema.jqassistant.org/rule/v1.10"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://schema.jqassistant.org/rule/v1.10 http://schema.jqassistant.org/rule/jqassistant-rule-v1.10.xsd">

    <concept id="cdi:Produces">
        <description>Abstract concept that returns Producer fields and Producer methods.
        </description>
        <cypher><![CDATA[
            MATCH
              (member:JEE:BeanProducer)
            RETURN
              member AS Producer
        ]]></cypher>
    </concept>

    <concept id="cdi:Disposes">
        <description>Returns all bean disposer methods.
        </description>
        <cypher><![CDATA[
            MATCH
              (disposeMethod:Method:BeanDisposer)
            RETURN
              disposeMethod AS DisposeMethod
        ]]></cypher>
    </concept>

    <concept id="cdi:Qualifier">
        <description>Abstract concept that returns all annotation types labeled with "CDI" and "Qualifier".
        </description>
        <cypher><![CDATA[
            MATCH
              (qualifier:Type:CDI:Qualifier)
            RETURN
              distinct qualifier AS Qualifier
        ]]></cypher>
    </concept>

    <concept id="cdi:Dependent">
        <description>Abstract concept that returns all beans, fields or methods labeled with "CDI" and "Dependent".
        </description>
        <cypher><![CDATA[
            MATCH
              (e:CDI:Dependent)
            RETURN
              e AS DependentElement
        ]]></cypher>
    </concept>

    <concept id="cdi:RequestScoped">
        <description>Abstract concept that returns all beans, fields or methods labeled with "CDI" and "RequestScoped".
        </description>
