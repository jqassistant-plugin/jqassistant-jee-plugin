<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant JEE Plugin" id="jqa.plugin.jee"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.jee.api.model.cdi.BeansXmlDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.cdi.CdiDescriptor</class>

        <class>org.jqassistant.plugin.jee.api.model.EnterpriseApplicationArchiveDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.WebApplicationArchiveDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.WebApplicationDirectoryDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.WebXmlDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.SessionConfigDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ErrorPageDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ServletDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ServletMappingDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.UrlPatternDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.IconDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.DescriptionDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.DisplayNameDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ParamValueDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.MultipartConfigDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.RunAsDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.SecurityRoleRefDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.FilterDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.FilterMappingDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.DispatcherDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ListenerDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.SecurityConstraintDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.AuthConstraintDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.UserDataConstraintDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.RoleNameDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.WebResourceCollectionDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.HttpMethodDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.HttpMethodOmissionDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.SecurityRoleDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.LoginConfigDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.FormLoginConfigDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ApplicationXmlDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.EjbModuleDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ConnectorModuleDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.ClientModuleDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.WebModuleDescriptor</class>

        <class>org.jqassistant.plugin.jee.api.model.jpa.PersistenceXmlDescriptor</class>
        <class>org.jqassistant.plugin.jee.api.model.jpa.PersistenceUnitDescriptor</class>

        <class>org.jqassistant.plugin.jee.api.model.MavenWebProjectDirectoryDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.jee.impl.scanner.cdi.BeansXmlScannerPlugin</class>

        <class>org.jqassistant.plugin.jee.impl.scanner.EarArchiveScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.WebApplicationArchiveScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.MavenWebProjectScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.WarClassesFileScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.WarClassesDirectoryScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.WebXmlScannerPlugin</class>
        <class>org.jqassistant.plugin.jee.impl.scanner.ApplicationXmlScannerPlugin</class>

        <class>org.jqassistant.plugin.jee.impl.scanner.jpa.PersistenceXmlScannerPlugin</class>
    </scanner>
    <rules>
        <resource>cdi/cdi.xml</resource>
        <resource>cdi/jakartaee-cdi.xml</resource>
        <resource>cdi/javaee-cdi.xml</resource>
        <resource>cdi/interceptor.xml</resource>
        <resource>cdi/jakartaee-interceptor.xml</resource>
        <resource>cdi/javaee-interceptor.xml</resource>
        <resource>ejb/ejb.xml</resource>
        <resource>ejb/jakartaee-ejb.xml</resource>
        <resource>ejb/javaee-ejb.xml</resource>
        <resource>jpa/jpa.xml</resource>
        <resource>jpa/jakartaee-jpa.xml</resource>
        <resource>jpa/javaee-jpa.xml</resource>
        <resource>injection/jee-injection.xml</resource>
        <resource>injection/jakartaee-injection.xml</resource>
        <resource>injection/javaee-injection.xml</resource>
        <resource>jsf/jsf.xml</resource>
        <resource>jsf/jakartaee-jsf.xml</resource>
        <resource>jsf/javaee-jsf.xml</resource>
        <resource>resource/jee-resource.xml</resource>
        <resource>resource/jakartaee-resource.xml</resource>
        <resource>resource/javaee-resource.xml</resource>
        <resource>transaction/jee-transaction.xml</resource>
        <resource>transaction/jakartaee-transaction.xml</resource>
        <resource>transaction/javaee-transaction.xml</resource>
    </rules>
</jqassistant-plugin>

